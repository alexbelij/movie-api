{
  "name": "movie-api",
  "version": "0.14.2",
  "description": "Get info for movies and TV shows",
  "repository": "anvilabs/movie-api",
  "homepage": "https://github.com/anvilabs/movie-api#readme",
  "bugs": {
    "url": "https://github.com/anvilabs/movie-api/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Ayan Yenbekbay",
    "email": "ayan.yenb@gmail.com",
    "url": "http://yenbekbay.me"
  },
  "engines": {
    "node": ">=6.0",
    "npm": ">=3.0"
  },
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "commit": "git-cz",
    "build": "yarn run clean && babel --out-dir ./lib --ignore **/__tests__,**/__mocks__ ./src && npm run build:flow",
    "build:flow": "find ./src -name '*.js' -not -path '*/__*' | while read filepath; do cp $filepath `echo $filepath | sed 's/\\/src\\//\\/lib\\//g'`.flow; done",
    "clean": "rimraf lib && mkdir lib",
    "prepublish": "yarn run build",
    "test": "npm-run-all --parallel typecheck lint jest",
    "test:ci": "npm-run-all --parallel typecheck lint jest:coverage && codecov",
    "jest": "jest",
    "jest:coverage": "jest --coverage",
    "lint": "eslint ./src",
    "typecheck": "flow ./src",
    "commitmsg": "validate-commit-msg",
    "prepush": "yarn test",
    "release": "standard-version"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "setupFiles": [
      "scripts/jest-env.js"
    ],
    "testEnvironment": "node",
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "testRegex": "/__tests__/.*-test\\.js$"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "maxSubjectLength": 72,
      "types": "conventional-commit-types"
    }
  },
  "dependencies": {
    "async-retry": "^0.2.1",
    "babel-runtime": "^6.20.0",
    "cheerio": "^0.22.0",
    "dataloader": "^1.2.0",
    "lodash.words": "^4.2.0",
    "moment": "^2.17.1",
    "parent-package-json": "^2.0.0",
    "promise-throttle": "^0.3.1",
    "ramda": "^0.22.1",
    "random-useragent": "^0.3.1",
    "request": "^2.79.0",
    "request-promise-native": "^1.0.3",
    "similarity": "^1.1.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.17.31",
    "@types/date-fns": "^2.5.3",
    "@types/dotenv": "^2.0.19",
    "@types/jest": "^16.0.2",
    "@types/moment": "^2.13.0",
    "@types/ramda": "^0.0.2",
    "@types/request": "^0.0.36",
    "@types/request-promise-native": "^1.0.2",
    "@types/rimraf": "^0.0.28",
    "@types/type-detect": "^0.1.28",
    "babel-cli": "^6.18.0",
    "babel-jest": "^18.0.0",
    "babel-plugin-ramda": "^1.1.6",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "codecov": "^1.0.1",
    "commitizen": "^2.9.2",
    "cz-conventional-changelog": "^1.2.0",
    "dotenv": "^2.0.0",
    "eslint": "^3.9.1",
    "eslint-config-anvilabs": "^2.1.0",
    "flow-bin": "^0.37.4",
    "husky": "^0.12.0",
    "jest": "^18.0.0",
    "npm-run-all": "^3.1.2",
    "rimraf": "^2.5.4",
    "standard-version": "^4.0.0",
    "type-detect": "^4.0.0",
    "validate-commit-msg": "^2.8.2"
  }
}
