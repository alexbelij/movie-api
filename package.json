{
  "name": "movie-api",
  "version": "0.0.0",
  "repository": "anvilabs/movie-api",
  "homepage": "https://github.com/anvilabs/movie-api#readme",
  "bugs": {
    "url": "https://github.com/anvilabs/movie-api/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Ayan Yenbekbay",
    "email": "ayan.yenb@gmail.com",
    "url": "http://yenbekbay.me"
  },
  "engines": {
    "node": ">=6.0",
    "npm": ">=3.0"
  },
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "commit": "git-cz",
    "build": "yarn run clean && babel --out-dir ./lib --ignore **/__tests__,**/__mocks__ ./src",
    "clean": "rimraf lib && mkdir lib",
    "prepublish": "yarn run build",
    "test": "npm-run-all --parallel lint typecheck jest",
    "jest": "jest",
    "typecheck": "flow ./src",
    "lint": "eslint ./src",
    "commitmsg": "validate-commit-msg",
    "prepush": "yarn test",
    "release": "standard-version"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "testEnvironment": "node",
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "testRegex": "/__tests__/.*-test\\.js$"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "maxSubjectLength": 72,
      "types": "conventional-commit-types"
    }
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "cheerio": "^0.22.0",
    "dataloader": "^1.2.0",
    "date-fns": "^1.10.0",
    "graphql-anywhere": "^0.2.4",
    "graphql-tag": "^0.1.16",
    "ramda": "^0.22.1",
    "request": "^2.76.0",
    "request-promise-native": "^1.0.3",
    "similarity": "^1.1.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.17.31",
    "@types/jest": "^15.1.32",
    "@types/ramda": "^0.0.2",
    "@types/request": "^0.0.32",
    "@types/request-promise-native": "^1.0.1",
    "babel-cli": "^6.18.0",
    "babel-jest": "^16.0.0",
    "babel-plugin-ramda": "^1.1.6",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "~3.8.1",
    "eslint-config-anvilabs": "^1.6.3",
    "flow-bin": "^0.34.0",
    "husky": "^0.11.9",
    "jest": "^16.0.2",
    "npm-run-all": "^3.1.1",
    "rimraf": "^2.5.4",
    "standard-version": "^3.0.0",
    "validate-commit-msg": "^2.8.2"
  }
}
